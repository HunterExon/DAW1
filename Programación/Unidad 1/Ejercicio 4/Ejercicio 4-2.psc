//Ejercicio4-2: Haz una variante del ejemplo 004b,
//usando el operador ">=".
Algoritmo ejercicio4_2
	primernumero ES ENTERO;
	ESCRIBIR "Dime un numero";
	LEER primernumero;
	
	Si primernumero >= 0 Entonces
		ESCRIBIR "Es mayor o igual que cero"; 
	SiNo
		ESCRIBIR "Es negativo";
	Fin Si
	
FinAlgoritmo
